{
  "name": "vscode-less",
  "displayName": "vscode-lessTheme",
  "description": "Advanced autocompletion and refactoring support for Less",
  "icon": "",
  "version": "0.0.6",
  "publisher": "ycjcl868",
  "homepage": "https://github.com/ycjcl868/vscode-less/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/ycjcl868/vscode-less"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "keywords": [
    "less",
    "autocompletion",
    "lessTheme"
  ],
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:less",
    "onLanguage:scss"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "vscode-lessTheme"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-less": "^3.1.4"
  }
}
